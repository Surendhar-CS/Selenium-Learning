<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE suite SYSTEM "https://testng.org/testng-1.0.dtd">
<suite name="Suite" >


	<groups>
		<run>
			<include name="ErrorHandlings">

			</include>
		</run>
	</groups>
<listeners>
<listener class-name="seleniumFrameworkPractice.refactor.TestComponents.Listeners"></listener>
</listeners>
	<test thread-count="5" name="ErrorValidations">
		<classes>
			<class
				name="seleniumFrameworkPractice.refactor.tests.ErrorValidations" />
		</classes>
	</test>


	<test thread-count="5" name="PlaceOrderTest">
		<classes>
			<class
				name="seleniumFrameworkPractice.refactor.tests.PlaceOrderTest" />
		</classes>
	</test> <!-- Test -->
</suite> <!-- Suite -->
